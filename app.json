{
  "expo": {
    "name": "MeLapse",
    "slug": "MeLapse",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/brand/icon.png",
    "userInterfaceStyle": "light",
    "privacy": "unlisted",
    "splash": {
      "image": "./assets/brand/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "red.artur.melapse"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/brand/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "red.artur.melapse"
    },
    "web": {
      "favicon": "./assets/brand/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f9fb43e2-8bd8-4516-aee2-58018f419ffc"
      }
    }
  },
  "plugins": [
    [
      "expo-build-properties",
      {
        "ios": {
          "useFrameworks": "static"
        }
      }
    ],
    [
      "expo-notifications",
      {
        "sounds": [
          "./local/assets/sounds/notification.wav"
        ]
      }
    ]
  ],
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-7754523043045564~1248185872"
  }
}
